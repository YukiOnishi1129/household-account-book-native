{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,tsx}'",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src"
  },
  "lint-staged": {
    "./src/**/*.{js,ts,tsx}": [
      "eslint --fix './src/**/*.{js,ts,tsx}'",
